{
    "autoDocstring.docstringFormat": "sphinx",
    "debug.allowBreakpointsEverywhere": true,
    "debug.inlineValues": true,
    "debug.toolBarLocation": "docked",
    "docker.languageserver.diagnostics.instructionWorkdirRelative": "error",
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    },
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.tabSize": 2,
    "editor.rulers": [
      88,
      100
    ],
    "editor.wordWrap": "off",
    "editor.wordWrapColumn": 88,
    "editor.suggestSelection": "first",
    "files.autoSave": "onWindowChange",
    "files.autoSaveDelay": 1000,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.views.repositories.files.layout": "tree",
    "markdown.extension.toc.githubCompatibility": true,
    "markdown.extension.toc.updateOnSave": true,
    "material-icon-theme.folders.theme": "specific",
    "materialTheme.accent": "Orange",
    "psi-header.changes-tracking": {
      "autoHeader": "autoSave",
      "enforceHeader": true,
      "exclude": [
        "md",
        "markdown",
        "json"
      ],
      "excludeGlob": [
        "out/**",
        "src/**/*.xyz"
      ],
      "include": [
        "python",
        "javascript"
      ],
    },
    "psi-header.config": {
      "blankLinesAfter": 1,
      "forceToTop": true,
      "license": "Custom"
    },
    "psi-header.lang-config": [
      {
        "beforeHeader": [],
        "begin": "",
        "blankLinesAfter": 0,
        "end": "",
        "language": "python",
        "prefix": "# ",
        "rootDirFileName": "pyproject.toml"
      },
     {
      "language": "javascript",
      "begin": "/**",
      "prefix": " * ",
      "end": " */",
      "blankLinesAfter": 1,
      "forceToTop": true,
    },
    ],
    "psi-header.license-text": [
      "All rights reserved."
    ],
    "psi-header.templates": [
      {
        "language": "*",
        "template": [
          "Copyright <<year>> <<company>>",
          "Author: <<author>>"
          // "<<filerelativepath>>",
          // "<<author>>, <<filecreated('YYYY-MM-DD')>>",
          // "Copyright (c) <<year>> <<company>>",
        ]
      }
    ],
    "psi-header.variables": [
      [
        "company",
        "Pempem Inc."
      ],
      [
        "author",
        "Philippe Guay"
      ],
      [
        "authoremail",
        "phil@pempem.org"
      ]
    ],
    "python.autoComplete.addBrackets": true,
    // "python.formatting.blackArgs": [
    //   "--line-length",
    //   "100"
    // ],
    "python.formatting.provider": "black",
    "python.jediEnabled": false,
    "python.linting.flake8Args": [
      "--max-line-length=88"
    ],
    "python.linting.flake8Enabled": false,
    "python.linting.lintOnSave": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
      "--max-line-length=88"
    ],
    "python.linting.prospectorArgs": [
      "-s",
      "veryhigh"
    ],
    "python.linting.pydocstyleArgs": [
      "--convention=numpy",
      "--ignore=D102"
    ],
    "python.linting.pylamaEnabled": true,
    "python.linting.pydocstyleEnabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
      "max-line-length=88",
      "--disable=bad-continuation,redefined-outer-name",
    ],
    "python.pythonPath": "/Users/philippeguay/.pyenv/shims/python",
    "python.sortImports.args": [
      "--recursive",
      "--atomic"
    ],
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [],
    "python.testing.unittestEnabled": false,
    "python.venvPath": "/Users/philippeguay/Library/Caches/pypoetry/virtualenvs/",
    "python.envFile": "${workspaceFolder}/.env",
    "restructuredtext.linter.extraArgs": [
      "--max-line-length=88"
    ],
    "rewrap.autoWrap.enabled": true,
    "search.exclude": {
      "**/.git/*": true,
      "**/.git/**": true,
      "**/__pycache__/**": true
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vs-kubernetes": {
      "vs-kubernetes.helm-path": "/Users/philippeguay/.vs-kubernetes/tools/helm/darwin-amd64/helm",
      "vs-kubernetes.draft-path": "/Users/philippeguay/.vs-kubernetes/tools/draft/darwin-amd64/draft",
      "vs-kubernetes.kubectl-path": "/Users/philippeguay/.vs-kubernetes/tools/kubectl/kubectl",
      "vs-kubernetes.minikube-path": "/Users/philippeguay/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
